result-view {
  match: LateMessage (result) {
    min (Required) max (One)
  }

  render {
    layout {
      layout-macro (late-message-details) {
        param (latemessage) {
          expression (result)
        }
      }
    }
  }
  conversation-drivers {
    conversation-driver {
      template ("Check the status of another train")
    }
  }
}